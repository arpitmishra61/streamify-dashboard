    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^22.1.0",
    "json-server": "^1.0.0-beta.3",
    "typescript": "^5.2.2",
    "vite": "^5.1.0"
